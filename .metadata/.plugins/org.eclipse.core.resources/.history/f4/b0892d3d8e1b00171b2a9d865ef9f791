import java.util.*;
import java.io.*;

public class CamperVan {
	public String depot;
	public int order;
	public String name;
	public String type;
	
	// CamperVan constructor
	public CamperVan(String depotBelong, String vanName, String vanType,) {
		this.depot = depotBelong;
		this.order = vanOrder;
		this.name = vanName;
		this.type = vanType;
	}
	
	// Returns name of van
	public String getVanName() {
		return this.name;
	}
}
